{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(index|form).css",
      "dest": "/public/$1.css"
    },
    {
      "src": "/src/(.*)",
      "dest": "/public/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}